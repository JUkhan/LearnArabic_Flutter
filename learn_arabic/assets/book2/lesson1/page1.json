      "words": [{ "w": "أُحِبُّ" }, { "w": "رَسُولَ" }, { "w": "اللَّهِ." }]
    },
    {
      "d": "ltr",
      "mode": "text",
      "words": [{ "w": "I love Arabic language." }]
